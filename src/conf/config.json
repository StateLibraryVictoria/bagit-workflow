{
    "id_parser" : {
        "sep": "[\\-\\._]?\\s?",
        "identifier_patterns": [
            ["(SC","\\d{4,}(?=[_-]|\\s|$))"],
            ["(RA","\\d{4}","\\d+(?=[_-]|\\s|$))"],
            ["(PA","\\d\\d)(\\d\\d)?(","\\d+(?=[_-]|\\s|$))"],
            ["(MS","\\d{2,}(?=[_-]|\\s|$))"],
            ["(PO","\\d+","slvdb)(?=[_-]|\\s|$)"],
            ["(POL",")?(\\d{3,}","slvdb)(?=[_-]|\\s|$)"],
            ["(POL","\\d{3,}{final})"],
            ["(H\\d\\d)(\\d\\d)?(","\\d+)(?=[_-]|\\s|$)"],
            ["(COMY\\d{5})(?=[_-]|\\s|$)"]
        ],
        "validation_patterns":[
            "SC\\d{4,}",
            "RA-\\d{4}-\\d+",
            "PA-\\d{2}-\\d+",
            "MS-?\\d{2,}",
            "POL-\\d{3,}",
            "(POL-)?\\d{3,}-slvdb",
            "H\\d{2}(\\d\\d)?-\\d+",
            "COMY\\d{5}"
        ],
        "normalisation_tests":[
            "(MS)(\\d+)", 
            "(SC)\\D?(\\d+)"
        ],
        "normalisation_joins":[
            "-", 
            ""
        ]
    }
}